  "layout": {
    "container": "max-w-7xl mx-auto px-4 sm:px-6 lg:px-8",
    "grid_system": "grid grid-cols-1 md:grid-cols-12 gap-6",
    "spacing": {
      "section": "py-16 md:py-24",
      "element": "space-y-6"
    },
    "sidebar": {
      "width": "w-64",
      "collapsed_width": "w-20",
      "style": "fixed left-0 top-0 h-full border-r border-border/40 bg-background/80 backdrop-blur-md z-40"
    }
  },
  "components": {
    "buttons": {
      "primary": "bg-primary text-primary-foreground hover:bg-primary/90 rounded-full px-8 py-6 font-medium transition-all hover:scale-[1.02] active:scale-[0.98]",
      "secondary": "bg-secondary text-secondary-foreground hover:bg-secondary/80 rounded-full px-8 py-6 font-medium",
      "ghost": "hover:bg-muted text-foreground rounded-full px-6 py-4"
    },
    "cards": {
      "clinical": "relative overflow-hidden rounded-2xl border border-border/50 bg-card/50 backdrop-blur-sm p-6 transition-all hover:border-primary/20 hover:shadow-lg",
      "highlight": "bg-gradient-to-br from-primary/5 to-accent/5 border border-primary/10"
    },
    "inputs": {
      "style": "h-12 rounded-xl border-border/50 bg-background/50 focus:ring-2 focus:ring-primary/20 focus:border-primary transition-all"
    }
  },
