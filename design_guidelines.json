{
  "identity": {
    "name": "VitalWave",
    "essence": "Premium Clinical Intelligence",
    "vibe": "Warm, Precise, Human-Centric, Editorial",
    "archetype": "The Empathetic Expert",
    "design_principles": [
      "Fusion over Imitation: Mix Clinical Precision with Spa-like Warmth",
      "Emotion First: Anxiety reduction through calm colors and soft motion",
      "Content is King: Data visualization must be instantly readable (The 'Health Passport' concept)",
      "Zoom In: High-fidelity details on medical imagery",
      "Negative Space: Generous padding to prevent cognitive overload"
    ]
  },
